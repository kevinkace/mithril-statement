  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^4.0.1",
    "minireset.css": "0.0.6",
    "mithril": "^2.0.4",
    "modular-css-webpack": "^15.0.0",
